{
  "compilerOptions": {
    "jsx": "react",
    "jsxFactory": "h",
    "jsxFragmentFactory": "Fragment",
    "outDir": "./dist/",
    "noImplicitAny": true,
    "module": "esnext",
    "target": "es5",
    "allowJs": true,
    "moduleResolution": "node",
    "downlevelIteration": true,
  },
  "include": ["src/**/*", "include", "example"],
}